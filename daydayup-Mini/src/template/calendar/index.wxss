/* pages/calendar/calendar.wxss */
.box {
  display: flex;
}

.box-lr {
  flex-direction: row;
}

.box-rl {
  flex-direction: row-reverse;
}

.box-tb {
  flex-direction: column;
}

.box-pack-center {
  justify-content: center;
}

.box-align-center {
  align-items: center;
}

.box-wrap {
  flex-wrap: wrap;
}

.flex {
  flex-grow: 1;
}

.bg {
  background-image: linear-gradient(to bottom, #faefe7, #ffcbd7);
  overflow: hidden;
}

.pink-color {
  color: #0038ee;
}

.white-color {
  color: #fff;
}

.fs24 {
  font-size: 24rpx;
}

.fs28 {
  font-size: 28rpx;
}

.fs32 {
  font-size: 32rpx;
}

.fs36 {
  font-size: 36rpx;
}

.top-handle {
  height: 80rpx;
}

.prev {
  text-align: right;
  height: 80rpx;
  
}

.next {
  height: 80rpx;
}

.prev-handle {
  width: 70rpx;
  height: 100%;
  color: white;
  
}

.next-handle {
  width: 70rpx;
  height: 100%;
  color: white;
}

.date-area {
  width: 190rpx;
  height: 80rpx;
  text-align: center;
  font-weight: bold;
  color: white;
}

.weeks {
  height: 50rpx;
  line-height: 50rpx;
  opacity: 0.5;
  color: black;
}

.week {
  text-align: center;
  font-weight: bold;
}

.days {
  height: 500rpx;
}

.grid {
  width: 14.285714285714285%;
}

.day {
  width: 60rpx;
  height: 60rpx;
  font-size: 26rpx;
  font-weight: 200;
}

.normal-day-color {
  color: #000000;
}

.day-choosed-color {
  color: #fff;
}

.todo-dot {
  width: 10rpx;
  height: 10rpx;
  border-radius: 50%;
  background-color: #cc5226;
}

.day-with-dot {
  height: 72rpx;
}

.disable-day-color {
  color: rgba(221, 221, 221, 0.637);
  
}

.disable-day-text {
 font-weight: 700;
}

.day-text {
  font-weight: 700;
}

.disable-day-circle {
  background-color: #fbfdff;
}

.border-radius {
  border-radius: 50%;
  position: relative;
  left: 0;
  top: 0;
}

.pink-bg {
  background-color:  #4797ff;
}

.purple-bg {
  background-color: #b8b8f1;
}

.right-triangle::after {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border: 15rpx solid transparent;
  border-left-color: #4797ff;
  position: absolute;
  right: -22rpx;
  top: 18rpx;
}

.left-triangle::before {
  content: "";
  display: block;
  width: 0;
  height: 0;
  border: 15rpx solid transparent;
  border-right-color: #4797ff;
  position: absolute;
  left: -22rpx;
  top: 18rpx;
}

.tips {
  text-align: center;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.types {
  background-color: #ffedf4;
  height: 50rpx;
}

.types-desc {
  padding: 0 20rpx;
}

.type-name {
  margin-top: 50rpx;
  margin-bottom: 30rpx;
}

.type-desc {
  padding: 0 35rpx;
  line-height: 38rpx;
}

.explain {
  border-top: 1px solid #eee;
  width: 90%;
  margin: 20rpx 5% 20rpx 5%;
  padding: 20rpx 0;
}

.explain-title {
  font-weight: bold;
  margin-bottom: 15rpx;
}

.explain-item {
  padding: 8rpx 20rpx;
  color: #fff;
}

.left-border-radius {
  border-top-left-radius: 20rpx;
  border-bottom-left-radius: 20rpx;
}

.right-border-radius {
  border-top-right-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}

/*button*/  
.btn {  
  width: 80%;  
  border-radius: 10rpx;  
  text-align: center;  
  margin: 40rpx 10%;  
  background: transparent;  
  color: #fff;  
}  
  
/*mask*/  
.drawer_screen {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  top: 0;  
  left: 0;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.5;  
  overflow: hidden;  
}  
  
/*content*/  
.drawer_box {  
  width: 650rpx;  
  overflow: hidden;  
  position: fixed;  
  top: 50%;  
  left: 0;  
  z-index: 1001;  
  background: #FAFAFA;  
  margin: -150px 50rpx 0 50rpx;  
  border-radius: 30px;  
}  
.title-style {
  border-radius: 5px;
}
.drawer_title{  
  padding:15px;  
  font: 14px "microsoft yahei";  
  text-align: center; 
  
  background-color: #0899f99e;

}  
.drawer_title_1{
  border-right: 2px solid rgb(35, 209, 240);
  border-radius: 30px 0 0 30px;
}

.drawer_title_2{
  border-radius: 0 30px 30px 0;
}


.drawer_content {  
  height: 110px;  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
}  
  
.btn_ok{  
  
  padding: 27rpx; 
  width: 327rpx; 
  font: 20px "microsoft yahei";  
  text-align: center;  
  border-top: 1px solid #E8E8EA;  
  color: #0899f99e;  
  font-weight: bold;
}  
  
