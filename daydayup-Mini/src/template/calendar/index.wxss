 .search-icon {
    width: 34rpx;
    height: 34rpx;
    left: 100rpx;
    position: fixed;
  }


  
  .item-content{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    justify-content: center;
    padding: 0 20rpx 0 20rpx;
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
    -webkit-transform: translateX(180rpx);
    transform: translateX(180rpx); 
    margin-left: -200rpx;
   
  }
  
  .list-item-touch-active .item-content, .list-item-touch-active .item-delete {
    -webkit-transform: translateX(0) !important;
    transform: translateX(0) !important;
  }
  .item-delete{
    width: 59rpx;
    height: 59rpx;
    line-height: 59rpx;
    
    display: flex;

    font-size: 32rpx;
    border-radius: 5px;
    border: 0 2px 0 0 solid #a1a1a100;
    text-align: center;
    z-index: 9999;
    justify-content: center;
    align-items: center;
    
    color: red;
    margin-top: -13rpx;
    font-size: 32rpx;
    -webkit-transform: translateX(180rpx);
    transform: translateX(180rpx);
    -webkit-transition: all 0.4s;
    transition: all 0.4s;
  }

  .list-item-touch-active .item-content{
    margin-left: -10rpx;
  }

