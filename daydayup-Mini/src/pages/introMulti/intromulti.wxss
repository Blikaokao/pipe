.init {
    margin-right: 20rpx;
    background-color: #eaf3fa;
    border-radius: 90rpx;
    border-color: #eaf3fa;
    border-style: solid;
    box-shadow: 11rpx 12rpx 19rpx -3rpx #a8d4f5;
    margin-top: 20rpx;
}

.initweb {
    margin-right: 20rpx;
    background : linear-gradient(to bottom right,#8f9453,#ffffff,#ffffff);
