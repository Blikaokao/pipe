    border-style: solid;
    box-shadow: 11rpx 12rpx 19rpx -3rpx #bfc76c;
    margin-top: 20rpx;
}

.initwatch {
    margin-right: 20rpx;
    background : linear-gradient(to right,#ffffff,#ffffff,#8eb3fa);
    border-radius: 90rpx;
    border-color: transparent;
    border-style: solid;
    box-shadow: 11rpx 12rpx 19rpx -3rpx #4f4cfc;
